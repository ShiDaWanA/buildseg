#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/HTML-study/code/Cesium-1.109/node_modules/.store/cloc@2.11.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/HTML-study/code/Cesium-1.109/node_modules/.store/cloc@2.11.0/node_modules"
fi
if [ -x "$basedir/perl" ]; then
  exec "$basedir/perl"  "$basedir/../.store/cloc@2.11.0/node_modules/cloc/lib/cloc" "$@"
else
  exec perl  "$basedir/../.store/cloc@2.11.0/node_modules/cloc/lib/cloc" "$@"
fi
