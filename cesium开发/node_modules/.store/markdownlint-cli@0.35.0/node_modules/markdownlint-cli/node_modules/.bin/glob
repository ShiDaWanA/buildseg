#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/HTML-study/code/Cesium-1.109/node_modules/.store/glob@10.2.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/HTML-study/code/Cesium-1.109/node_modules/.store/glob@10.2.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../glob@10.2.7/node_modules/glob/dist/cjs/src/bin.js" "$@"
else
  exec node  "$basedir/../../../../../glob@10.2.7/node_modules/glob/dist/cjs/src/bin.js" "$@"
fi
